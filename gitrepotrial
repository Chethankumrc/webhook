pipeline {
    agent any
    
    environment{
    PATH = "/opt/apache-maven/bin:$PATH"
}
    stages {
        stage('git checkout') {
            steps {
                git url: 'https://github.com/Chethankumrc/webhook.git'
            }
        }
    
        stage('Maven build') {
            steps {
                sh "mvn clean install"
            }
        }
    
    }
}
